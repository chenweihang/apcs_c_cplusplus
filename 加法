/* printf example */
#include <stdio.h>
#include <stdlib.h>
#include <limits.h>
#include <float.h>
int main() {
    int a, b;
    printf("A=> ");
    scanf("%d",&a);
    printf("B=> ");
    scanf("%d",&b);
    printf("A+B=%d\n", a+b);
    return 0;
}
int main() {
    int a, b;
    printf("A=> ");
    scanf("%d",&a);
    printf("B=> ");
    scanf("%d",&b);
    printf("A-B=%d\n", a+b);
    return 0;
}
int main() {
    int a, b;
    printf("A=> ");
    scanf("%d",&a);
    printf("B=> ");
    scanf("%d",&b);
    printf("A*B=%d\n", a+b);
    return 0;
}
int main() {
    int a, b;
    printf("A=> ");
    scanf("%d",&a);
    printf("B=> ");
    scanf("%d",&b);
    printf("A/B=%d\n", a+b);
    return 0;
}
int main() {
    int a, b;
    printf("A=> ");
    scanf("%d",&a);
    printf("B=> ");
    scanf("%d",&b);
    printf("A%B=%d\n", a % b);
    return 0;
}
